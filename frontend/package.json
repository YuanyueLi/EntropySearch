{
  "name": "entropy_search",
  "version": "0.1.0",
  "description": "GUI for Entropy Search",
  "main": "public/electron.js",
  "private": false,
  "homepage": ".",
  "author": "Yuanyue Li",
  "license": "MIT",
  "build": {
    "win": {
      "icon": "build/icon.png",
      "target": [
        {
          "target": "portable"
        }
      ]
    },
    "extraFiles": [
      {
        "from": "../backend/dist/",
        "to": ".",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "scripts": {
    "electron-dev": "electron .",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "ahooks": "^2.10.11",
    "antd": "^4.16.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@craco/craco": "^6.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "concurrently": "^6.3.0",
    "craco-less": "^1.20.0",
    "electron": "^15.1.2",
    "electron-builder": "^22.13.1",
    "electron-squirrel-startup": "^1.0.0",
    "wait-on": "^6.0.0",
    "web-vitals": "^1.1.2"
  }
}