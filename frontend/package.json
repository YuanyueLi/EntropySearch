{
  "name": "entropy_search",
  "version": "0.3.0",
  "description": "GUI for Entropy Search",
  "author": {
    "name": "Yuanyue Li"
  },
  "license": "Apache-2.0",
  "homepage": ".",
  "main": "public/electron.js",
  "build": {
    "productName": "EntropySearch",
    "directories": {
      "buildResources": "assets"
    },
    "extraFiles": [
      {
        "from": "../backend/dist/",
        "to": ".",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.productivity",
      "icon": "build/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "category": "Science",
      "icon": "build/icon.png"
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.png"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@craco/craco": "^7.1.0",
    "ahooks": "^3.7.7",
    "antd": "5.4.7",
    "axios": "^1.4.0",
    "jotai": "^2.1.0",
    "plotly.js": "^2.23.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "smiles-drawer": "2.0.3",
    "virtuallist-antd": "^0.7.6",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "pack": "craco build && electron-builder --dir",
    "dist": "craco build && electron-builder",
    "electron": "electron .",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "electron": "24.3.0",
    "electron-builder": "23.6.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
